<resources>
    <!-- Color of the UDFPS pressed view -->
    <color name="config_udfpsColor">#ffffffff</color>

    <!-- Whether to enable framework dimming for UDFPS -->
    <bool name="config_udfpsFrameworkDimming">false</bool>

    <!-- Array of brightness-alpha LUT for framework dimming -->
    <string-array name="config_udfpsDimmingBrightnessAlphaArray" translatable="false">
          <!-- Example:
          <item>0,255</item>
          <item>1,234</item>
          <item>3,227</item>
          <item>8,208</item>
          <item>16,192</item>
          <item>27,176</item>
          <item>41,160</item>
          <item>61,144</item>
          <item>80,128</item>
          <item>104,112</item>
          <item>130,96</item>
          <item>158,80</item>
          <item>188,64</item>
          <item>221,48</item>
          <item>250,36</item>
          <item>255,33</item>
          -->
    </string-array>

    <!-- Brightness range min for UDFPS dimming -->
    <integer name="config_udfpsDimmingBrightnessMin">0</integer>

    <!-- Brightness range max for UDFPS dimming -->
    <integer name="config_udfpsDimmingBrightnessMax">0</integer>

    <!-- The amount of frames to delay by when disabling the dim layer.
         This is used to prevent flickers due to the dimming being disabled
         before the screen has had chance to switch out of HBM mode -->
    <integer name="config_udfpsDimmingDisableDelay">0</integer>

    <!-- The same as above, for enrollment in Settings -->
    <integer name="config_udfpsEnrollingDimmingDisableDelay">0</integer>

    <!-- StatusBar burn-in protection -->
    <bool name="config_statusBarBurnInProtection">true</bool>
    <integer name="config_shift_interval">60</integer>

    <!-- Whether usage of the proximity sensor during doze is supported -->
    <bool name="doze_proximity_sensor_supported">true</bool>

    <!-- Doze: does the double tap sensor need a proximity check? -->
    <bool name="doze_double_tap_proximity_check">false</bool>

    <!-- Doze: does the long press sensor need a proximity check? -->
    <bool name="doze_long_press_proximity_check">false</bool>

    <!-- If not zero, limits the internal screen recorder's framerate to the set value. -->
    <integer name="config_screenRecorderMaxFramerate">0</integer>

    <!-- If not empty, sets the AVC codec profile level used for the internal screen recorder.
         Possible values: 3, 3.1, 3.2, 4, 4.1, 4.2
    -->
    <string name="config_screenRecorderAVCProfileLevel" translatable="false">4.2</string>

    <!-- Same as above but for medium setting -->
    <string name="config_screenRecorderAVCProfileLevelMedium" translatable="false">4</string>

    <!-- Same as above but for low setting -->
    <string name="config_screenRecorderAVCProfileLevelLow" translatable="false">3.2</string>

    <!-- If not empty, sets the HEVC codec profile level used for the internal screen recorder.
         Possible values: 3, 3.1, 4, 4.1 add "h" at the end to indicate usage of high profile
         Otherwise uses main
    -->
    <string name="config_screenRecorderHEVCProfileLevel" translatable="false">4.1h</string>

    <!-- Same as above but for medium setting -->
    <string name="config_screenRecorderHEVCProfileLevelMedium" translatable="false">4</string>

    <!-- Same as above but for low setting -->
    <string name="config_screenRecorderHEVCProfileLevelLow" translatable="false">3.1</string>

    <!-- Allow devices override audio panel location to the left side -->
    <bool name="config_audioPanelOnLeftSide">false</bool>

    <!-- Whether to enable HEVC toggle for screenrecord -->
    <bool name="config_screenRecordHEVC">true</bool>

</resources>
