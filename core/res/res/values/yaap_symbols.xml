<resources>
    <!-- Device keyhandlers -->
    <java-symbol type="array" name="config_deviceKeyHandlerLibs" />
    <java-symbol type="array" name="config_deviceKeyHandlerClasses" />

    <!-- PixelPropUtils -->
    <java-symbol type="string" name="cert_device" />
    <java-symbol type="string" name="cert_fp" />
    <java-symbol type="string" name="cert_model" />
    <java-symbol type="string" name="cert_spl" />
    <java-symbol type="integer" name="cert_sdk" />
    <java-symbol type="string" name="cert_manufacturer" />

     <!-- Package Manager -->
     <java-symbol type="array" name="config_deviceDisabledComponents" />
     <java-symbol type="array" name="config_globallyDisabledComponents" />
     <java-symbol type="array" name="config_forceEnabledComponents" />

    <java-symbol type="array" name="config_loggersPkgList" />

    <!-- Display color inversion availability -->
    <java-symbol type="bool" name="config_displayInversionAvailable" />

    <!-- Camera -->
    <java-symbol type="array" name="config_cameraAuxPackageAllowList" />
    <java-symbol type="array" name="config_cameraAuxPackageBlackList" />

    <!-- Absolute path of the DC dimming node -->
    <java-symbol type="string" name="config_dcdNodePath" />

    <!-- Power menu reboot -->
    <java-symbol type="string" name="reboot_title" />
    <java-symbol type="string" name="reboot_confirm" />
    <java-symbol name="reboot_to_bootloader_title" type="string" />
    <java-symbol name="reboot_to_bootloader_message" type="string" />
    <java-symbol name="reboot_to_fastboot_title" type="string" />
    <java-symbol name="reboot_to_fastboot_message" type="string" />
    <java-symbol name="reboot_to_recovery_title" type="string" />
    <java-symbol name="reboot_to_recovery_message" type="string" />
    <java-symbol name="reboot_system_title" type="string" />
    <java-symbol name="reboot_system_message" type="string" />
    <java-symbol type="array" name="config_rebootActionsList" />

    <!-- Pocket Lock -->
    <java-symbol type="layout" name="pocket_lock_view" />
    <java-symbol type="bool" name="config_pocketModeSupported" />

    <!-- Pocket bridge -->
    <java-symbol type="string" name="config_pocketBridgeSysfsInpocket" />
    <!-- Pocket Service -->
    <java-symbol type="string" name="config_pocketJudgeVendorSensorName" />

    <!-- Cycle through ringer modes -->
    <java-symbol type="string" name="volume_dialog_ringer_guidance_normal" />

    <!-- Title & summary of notification letting users know that battery saver is now on -->
    <java-symbol type="string" name="battery_saver_on_notification_title" />
    <java-symbol type="string" name="battery_saver_on_notification_summary" />

    <!-- Delay in ms before invoking single tap gesture
         Should be set to dt2w max time / 2 when needed -->
    <java-symbol type="integer" name="config_singleTapDelay" />

    <!-- PowerOffAlarmService -->
    <java-symbol type="string" name="notification_channel_poweroff_alarm" />
    <java-symbol type="string" name="poweroff_alarm_title" />
    <java-symbol type="string" name="poweroff_alarm_body" />
    <java-symbol type="drawable" name="ic_alarm_on" />

    <!-- Slider vibrations fallbacks - set either to non positive to disable
         maximum duration in ms the slider vibrations scale to from 0 -->
    <java-symbol type="integer" name="config_sliderVibFallbackDuration" />

    <!-- Landscape Battery -->
    <java-symbol type="string" name="config_batterymeterRLandPerimeterPath" />
    <java-symbol type="string" name="config_batterymeterRLandFillMask" />
    <java-symbol type="string" name="config_batterymeterRLandBoltPath" />
    <java-symbol type="string" name="config_batterymeterRLandPowersavePath" />
    <java-symbol type="string" name="config_batterymeterRLandErrorPerimeterPath" />
    <java-symbol type="string" name="config_batterymeterLandPerimeterPath" />
    <java-symbol type="string" name="config_batterymeterLandFillMask" />
    <java-symbol type="string" name="config_batterymeterLandBoltPath" />
    <java-symbol type="string" name="config_batterymeterLandPowersavePath" />
    <java-symbol type="string" name="config_batterymeterLandErrorPerimeterPath" />
</resources>
